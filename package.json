{
    "name": "codely",
    "displayName": "codely",
    "description": "opens the clipboard path in the editor",
    "version": "0.0.1",
    "publisher": "hollerith",
    "repository": {
        "type": "git",
        "url": "https://github.com/hollerith/codely.git"
    },
    "engines": {
        "vscode": "^1.79.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.openStringLocation",
                "title": "Open String Location"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "package": "vsce package"
    },
    "devDependencies": {
        "@types/vscode": "^1.79.0",
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "typescript": "^4.9.3",
        "@vscode/test-electron": "^2.2.0",
        "vsce": "^1.110.1"
    }
}